{
  "name": "cardverse-api",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "deploy": "NODE_PATH=./dist node ./dist/main.js",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "postbuild": "npm run copy-pm2 && npm run copy-packagejson && npm run copy-config && npm run copy-assets && npm run copy-certs",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "assignment-evaluation-queue:start": "ts-node -r tsconfig-paths/register src/queues/workers/assignment-evaluation-queue.worker.ts",
    "start:dev": "nodemon --exec \"ts-node -r tsconfig-paths/register src/main.ts\"",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/src/main.js\"",
    "start:prod": "node dist/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "copy-pm2": "copyfiles ./execute/*.* ./dist/",
    "copy-packagejson": "copyfiles ./package.json ./dist/",
    "copy-config": "copyfiles ./.env* ./dist/",
    "copy-assets": "copyfiles ./assets/**/*.* ./dist/",
    "copy-certs": "copyfiles --flat ./src/certs/**/*.* ./dist/certs/",
    "startdevqa": "pm2 start execute/process.DevQA.json",
    "startprodrelease": "pm2 start execute/process.Release.json --no-daemon",
    "stopdevqa": "pm2 stop execute/process.DevQA.json",
    "stopprodrelease": "pm2 stop execute/process.Release.json",
    "prestartdevqa": "npm install",
    "prestartprodrelease": "npm install --force",
    "seed:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js seed -r src/database/config -n typeorm.config.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.821.0",
    "@aws-sdk/s3-request-presigner": "^3.821.0",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/bull": "^11.0.2",
    "@nestjs/common": "11.0.13",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "11.0.13",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "11.0.13",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/swagger": "^11.1.1",
    "@nestjs/testing": "11.0.13",
    "@nestjs/typeorm": "^11.0.0",
    "@supabase/supabase-js": "^2.49.4",
    "amqplib": "^0.10.8",
    "await-to-js": "^3.0.0",
    "axios": "^1.8.4",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "body-parser": "^2.2.0",
    "bull": "^4.16.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.10",
    "email-templates": "^12.0.2",
    "expo-server-sdk": "^3.15.0",
    "express": "^5.1.0",
    "fs": "^0.0.1-security",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "http": "0.0.1-security",
    "https": "^1.0.0",
    "kafkajs": "^2.2.4",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "nest-puppeteer": "^1.1.1",
    "nestjs-seeder": "^0.3.2",
    "nodemailer": "^6.10.0",
    "nodemailer-sendgrid": "^1.0.3",
    "passport": "^0.7.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.1",
    "pg": "^8.14.1",
    "puppeteer": "^21.0.1",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.2",
    "stompit": "^1.0.0",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.22",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.6",
    "@nestjs/schematics": "^11.0.3",
    "@types/amqplib": "^0.10.7",
    "@types/bull": "^4.10.4",
    "@types/email-templates": "^10.0.4",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/joi": "^17.2.3",
    "@types/node": "^22.14.0",
    "@types/stompit": "^0.26.6",
    "@types/superagent": "^8.1.9",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@types/validator": "^13.12.3",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "copyfiles": "^2.4.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "fastify-swagger": "^5.2.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.1",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.2.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
